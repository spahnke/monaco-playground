{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "dev",
			"isBackground": true,
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": {
				"base": "$tsc-watch",
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^rollup",
					"endsPattern": "waiting for changes\\.\\.\\.$|^\\[!\\]"
				}
			}
		},
		{
			"label": "serve",
			"type": "npm",
			"script": "serve",
			"isBackground": true,
			"problemMatcher": {
				"owner": "custom",
				"pattern": {
					"regexp": "_____________________"
				},
				"background": {
					"beginsPattern": "^Starting up",
					"endsPattern": "^Available on"
				}
			}
		}
	]
}